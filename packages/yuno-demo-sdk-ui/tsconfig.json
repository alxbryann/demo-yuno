{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "esnext",
    "target": "es2020",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "lib": ["es2020", "dom"],
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
